  "extends": "eslint:recommended",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
    "plugins": [
      "react"
    ],
    "rules": {
    "react/jsx-filename-extension": 0,
    "react/forbid-prop-types": 0
  },

